pipeline {
    agent any
    stages {
        stage('Compile-Package') {
        steps {
            withMaven(maven : 'maven-3'){
            sh 'mvn clean compile'}
            }
        }
        stage('Testing Scope') {
        steps {
            withMaven(maven : 'maven-3'){
            sh 'mvn test'}
            }
        }
    }
}