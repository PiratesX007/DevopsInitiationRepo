    pipeline {
        agent any
        tools {
            maven "maven-m3"
        }

        stages {
            stage('Compile Stage') {
                steps {
                    git 'https://github.com/PiratesX007/DevopsInitiationRepo'
                    bat 'mvn -Dmaven.test.failure.ignore=true clean package'
                }

            }
        }
    }